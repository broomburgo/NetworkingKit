// MARK: - Standard initializer for structs
{% for type in types.structs|annotated:"init" %}

extension {{ type.name }} {
    init({% for var in type.variables|!static|!computed %}{{ var.name }}: {{ var.actualTypeName }}{% if not forloop.last %},{% endif %}{% endfor %}) {
        {% for var in type.variables|!static|!computed %}
        self.{{ var.name }} = {{ var.name }}
        {% endfor %}
    }
}
{% endfor %}
